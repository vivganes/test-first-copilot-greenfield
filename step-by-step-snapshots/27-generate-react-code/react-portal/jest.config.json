{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
      }
    }
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/e2e/"
  ]
}
